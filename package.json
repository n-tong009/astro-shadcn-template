{
  "name": "astro-shadcn-template",
  "type": "module",
  "version": "0.1.0",
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.1.0"
  },
  "volta": {
    "node": "24.4.1",
    "npm": "10.8.2"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development astro dev --mode development",
    "start": "astro dev",
    "build": "astro build && npm run format-html",
    "format-html": "prettier --write \"dist/**/*.html\"",
    "preview": "astro preview",
    "astro": "astro",
    "stg": "cross-env NODE_ENV=staging astro build --mode staging",
    "prod": "cross-env NODE_ENV=production astro build --mode production",
    "lint": "eslint . --ext .js,.mjs,.ts,.jsx,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.mjs,.ts,.jsx,.tsx,.astro --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "build:github": "cross-env PUBLIC_BASE_URL=/astro-shadcn-template/ pnpm build"
  },
  "dependencies": {
    "@astrojs/react": "^4.2.5",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-radio-group": "^1.3.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.6",
    "@tailwindcss/vite": "^4.1.4",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.2",
    "astro": "^5.12.9",
    "chalk": "^5.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.539.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.0",
    "sass-embedded": "^1.90.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@sentry/node": "^10.5.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.2.1",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^10.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "fast-glob": "^3.3.3",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tw-animate-css": "^1.3.6",
    "vitest": "^3.2.4"
  }
}
